ese (quark)"},{"isHeader":false,"content":"15 g cornflour"},{"isHeader":false,"content":"2 pinches fine sea salt"},{"isHeader":false,"content":"2 pinches ground black pepper"},{"isHeader":false,"content":"6 fresh basil leaves"},{"isHeader":false,"content":"4 egg yolks, from large eggs"}],"nutrition":["per 1 portion","971 kJ \/ 233 kcal","15 g","6 g","16 g"],"hintsAndTricks":[],"basicInfo":["easy","15m","35m","6 portions"]}