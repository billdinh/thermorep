ese and mint","ingredients":[{"isHeader":true,"content":"Ingredients"},{"isHeader":true,"content":"Dough"},{"isHeader":false,"content":"250 g full cream milk"},{"isHeader":false,"content":"80 g olive oil"},{"isHeader":false,"content":"1 ½ tsp dried instant yeast"},{"isHeader":false,"content":"350 g baker's flour, plus extra for dusting"},{"isHeader":false,"content":"2 tsp sugar"},{"isHeader":false,"content":"1 tsp salt"},{"isHeader":true,"content":"Filling"},{"isHeader":false,"content":"6 - 8 sprigs fresh mint, leaves only"},{"isHeader":false,"content":"100 g Parmesan cheese, crust removed and cut into pieces (3 cm)"},{"isHeader":false,"content":"160 g cream cheese"},{"isHeader":false,"content":"3 tsp nigella seeds"},{"isHeader":false,"content":"plain flour, for dusting"}],"nutrition":["per 1 portions","1125.8 kJ \/ 268 kcal","8.6 g","24.5 g","14.9 g"],"hintsAndTricks":[],"basicInfo":["easy","20m","2h 15m","12 portions"]}