  <script>
    (function () {
      !function (e, a, t, n, s, i, c) {
      e.AlgoliaAnalyticsObject = s, e.aa = e.aa || function () { (e.aa.queue = e.aa.queue || []).push(arguments) }, i = a.createElement(t),
      c = a.getElementsByTagName(t)[0], i.async = 1, i.src = "https://searchpublic-search.external.eu-tm-prod.vorwerk-digital.com/searchinsights@2.0.1.min-f7f37597a9f1309a261c13b1550e019e.js", c.parentNode.insertBefore(i, c)
      }(window, document, "script", 0, "aa");
  
      function clickHandler (e) {
        const tile = e.target.closest('core-tile')
        if (e.target.matches('core-tile .core-tile__trigger') || e.target.matches('core-tile > .link--alt *')) {
          window.aa("clickedObjectIDsAfterSearch", {
            index: tile.getAttribute('data-index'),
            eventName: "click-result",
            objectIDs: [tile.getAttribute('id')],
            queryID: tile.getAttribute('data-queryid'),
            positions: [parseInt(tile.getAttribute('data-position'))]
          })
        } else if (e.target.matches('core-tile > core-context-menu *')) {
          window.aa('convertedObjectIDsAfterSearch', {
            index: tile.getAttribute('data-index'),
            eventName: 'conversion',
            queryID: tile.getAttribute('data-queryid'),
            objectIDs: [tile.getAttribute('id')]
          })
        }
      }
  
      function _aaInit (opts = {}) {
        const algoliaAPI = document.getElementById('algoliaApi')
  
        window.aa('init', {
          appId: algoliaAPI.getAttribute('app-id'),
          apiKey: algoliaAPI.getAttribute('client-key'),
          ...opts
        });
      }
  
      function initAlgoliaTracking () {
        _aaInit({ userHasOptedOut: false, useCookie: true })
        document.addEventListener('click', clickHandler)
      }
  
  
      let algoliaInsightsInitDone = false
  
      function initAlgoliaTrackingOnWeb () {
        if (algoliaInsightsInitDone) return
  
        algoliaInsightsInitDone = true
        initAlgoliaTracking()
      }
  
      function deinitAlgoliaTracking () {
        document.removeEventListener('click', clickHandler)
        _aaInit({ userHasOptedOut: true, useCookie: false })
        document.cookie = "_ALGOLIA=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/"
        algoliaInsightsInitDone = false
      }
  
      function isAlgoliaAllowed() {
        const ALGOLIA_COOKIE_GROUP = 'C0003'
        return window.OnetrustActiveGroups &&
          window.OnetrustActiveGroups.indexOf(ALGOLIA_COOKIE_GROUP) >= 0
      }
  
      window.OptanonWrapper = function OptanonWrapper() {
        if (isAlgoliaAllowed()) initAlgoliaTrackingOnWeb()
        else deinitAlgoliaTracking()
      }
  
      if (isAlgoliaAllowed()) {
        initAlgoliaTrackingOnWeb()
      }
    })()
  </script>


<script>
      const mobileTrackingCookieName = 'cookidooMobileAnalyticsEnabled';
      const performanceCookieGroup = 'C0002';
      const marketingCookieGroup = 'C0004';

      function getCookie(cName) {
        const name = cName + "=";
        const cDecoded = decodeURIComponent(document.cookie);
        const cArr = cDecoded.split('; ');
        let res;
        cArr.forEach(val => {
          if (val.indexOf(name) === 0) res = val.substring(name.length);
        })
        return res
      }

      function enableAnalyticsStorageConsent() {
        gtag('consent', 'update', { 'analytics_storage': 'granted' });
      }

      function disableAnalyticsStorageConsent() {
        gtag('consent', 'update', { 'analytics_storage': 'denied' });
      }

      function enableAdsStorageConsent() {
        gtag('consent', 'update', { 'ad_storage': 'granted' });
      }

      function disableAdsStorageConsent() {
        gtag('consent', 'update', { 'ad_storage': 'denied' });
      }

      function setMobileTracking() {
        const mobileTrackingConsentValue = getCookie(mobileTrackingCookieName);

        if (mobileTrackingConsentValue) {
          if (mobileTrackingConsentValue === 'true') {
            enableAdsStorageConsent();
            enableAnalyticsStorageConsent();
          }

          if (mobileTrackingConsentValue === 'false') {
            disableAdsStorageConsent();
            disableAnalyticsStorageConsent();
          }
        }
      }

      function setWebTracking(cookieGroups) {
        if (cookieGroups) {
          if (cookieGroups.includes(performanceCookieGroup)) {
            enableAnalyticsStorageConsent();
          } else {
            disableAnalyticsStorageConsent();
          }

          if (cookieGroups.includes(marketingCookieGroup)) {
            enableAdsStorageConsent();
          } else {
            disableAdsStorageConsent();
          }
        }
      }

      function attachOnClickGaEvents() {
        document.querySelectorAll('[data-ga-event-action]').forEach(node => {
          // Collect attributes data
          const eventAction = node.attributes['data-ga-event-action'] && node.attributes['data-ga-event-action'].value;
          const eventCategory = node.attributes['data-ga-event-category'] && node.attributes['data-ga-event-category'].value;
          const eventLabel = node.attributes['data-ga-event-label'] && node.attributes['data-ga-event-label'].value;
          const eventValue = node.attributes['data-ga-event-value'] && node.attributes['data-ga-event-value'].value;

          // Validate
          if (!eventAction.length) throw Error('Missing ga-event-action value');

          // Prepare event details
          const details = {};
          if (eventCategory) details['event_category'] = eventCategory;
          if (eventLabel) details['event_label'] = eventLabel;
          if (eventValue) details['value'] = eventValue;

          // Attach click event to gtag
          node.addEventListener('click', function() {
            gtag('event', eventAction, details);
          });
        });
      }

      setMobileTracking();
      setWebTracking(window.OnetrustActiveGroups);

      window.addEventListener('consentChange', function ({ detail }) {
        setWebTracking(detail.onetrustActiveGroups);
      });

      window.addEventListener('cookidooMobileAnalyticsChanged', function () {
        setMobileTracking();
      });

      attachOnClickGaEvents()
    </script><div id="onetrust-consent-sdk"><div class="onetrust-pc-dark-filter ot-hide ot-fade-in"></div></div></body></html>